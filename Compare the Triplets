vector<int> compareTriplets(vector<int> a, vector<int> b) {
    
    vector<int> result;
    int alice = 0;
    int bob = 0;
    for(int i=0;i<a.size();i++){
        if(a[i] > b[i]) 
            aliceScore ++;
        else if(b[i] > a[i])
            bobScore++;
        }         
        result.push_back(alice);
        result.push_back(bob);
    return result;
}
